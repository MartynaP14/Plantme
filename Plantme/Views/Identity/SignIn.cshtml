
@model LogInObject
@{
    ViewData["Title"] = "SignIn";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>SignIn</h1>




<h4>Please enter your details</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method = "post" asp-controller="Identity" asp-action="SingIn">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>


            <div class="form-group">
                <label asp-for="LogInEmail" class="control-label"></label>
                <input asp-for="LogInEmail" class="form-control" />
                <span asp-validation-for="LogInEmail" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="LogInPassword" class="control-label"></label>
                <input asp-for="LogInPassword" class="form-control" />
                <span asp-validation-for="LogInPassword" class="text-danger"></span>
            </div>

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


